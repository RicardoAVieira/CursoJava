package fundamentos;

public class Inferencia {
	public static void main(String[] args) {
		
//		O conceito de inferência, o java "identifica" qual é tipo da variavel.
		
		double a = 4.5;
		System.out.println(a);
		
		var b =4.5;
		System.out.println(b);
		
		var c = "Olá Mundo !";
		System.out.println(c);
		
	}
}
